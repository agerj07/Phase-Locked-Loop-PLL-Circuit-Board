void printBin(byte aByte) {   //function to print binary
  for (int8_t aBit = 7; aBit >= 0; aBit--)
    Serial.print(bitRead(aByte, aBit) ? '1' : '0');
}


void writeEEPROM(byte control, byte dataAdd, byte data){    //writes to the eeprom
  //blockSelect = dataAdd & (0b11100000);
  //blockSelect = blockSelect >> 4;
  //control |= blockSelect;

  byte highAdd;
  byte lowAdd;

  highAdd = (dataAdd >> 8) & 0xFF;
  lowAdd = dataAdd & 0xFF;
    
  Serial.print("Control byte: ");
  printBin(control);
  Serial.print(". Data address: ");
  printBin(dataAdd);
  Serial.print(". Setting write: ");
  Serial.print(data,HEX);
    
  Wire.beginTransmission(EEPROM_ADDRESS);
  Wire.write(control);
  Wire.write(highAdd);
  Wire.write(lowAdd);
  Wire.write(data);
  byte transmissionStatus = Wire.endTransmission();
    
  Serial.print(". Write status: ");   //checks the status of the write
  Serial.println(transmissionStatus);
  delay(10);
}


void readEEPROM(byte Control, byte dataAdd){    //read the eeprom register just written to (current read)
  
  
  
  Wire.beginTransmission(EEPROM_ADDRESS);
  Wire.write(Control);

  byte nackStat = Wire.endTransmission(false); //keep the connection active for reading
  Serial.print("Read control byte. Status: ");
  Serial.println(nackStat);

  if(nackStat == 0){
      Wire.requestFrom(EEPROM_ADDRESS, 1); //request one byte from EEPROM
      
      while(Wire.available()){
      
        byte dataRead = Wire.read(); //read the received byte
        Serial.print("Data read at address 0x");
        Serial.print(dataAdd, HEX);
        Serial.print(". Value: ");
        Serial.println(dataRead, HEX);
      }
      nackStat = Wire.endTransmission();
  
      delay(10);
  }  
}

