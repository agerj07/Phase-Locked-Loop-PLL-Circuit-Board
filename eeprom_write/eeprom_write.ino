      char c = Serial.read();
  }
  if(answer == 'y' || answer == 'Y') {
    Serial.println("Enter desired divider value: ");
    
    while(Serial.available() == 0){}
    while(Serial.available() > 0){
      divider = Serial.parseInt();    //read integer
      while(!divider){}  
      
      //delay(750);
      Serial.println(divider);
      
      //take the input and place in temporary registers
      divReg1 = divider & 0xFF;
      divReg2 = (divider >> 8) & 0xFF;
      divReg3 = (divider >> 16) & 0xFF;
      divReg4 = divider >> 24;
    
      
    }  
  }
  else{
    Serial.println("Output Frequency not changed");
  }

    // Clear any remaining characters in the input buffer
    while(Serial.available() > 0) {
      char c = Serial.read();
    }
  
}







